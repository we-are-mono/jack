{
  "interfaces": {
    "eth0": {
      "type": "physical",
      "device": "eth0",
      "enabled": true
    },
    "eth1": {
      "type": "physical",
      "device": "eth1",
      "enabled": true
    },
    "eth2": {
      "type": "physical",
      "device": "eth2",
      "enabled": true
    },
    "br-lan": {
      "type": "bridge",
      "device": "br-lan",
      "enabled": true,
      "members": ["eth1", "eth2"],
      "protocol": "static",
      "ipaddr": "192.168.1.1",
      "netmask": "255.255.255.0",
      "mtu": 1500
    },
    "vlan100": {
      "type": "vlan",
      "device": "vlan100",
      "enabled": true,
      "parent": "eth0",
      "vlan_id": 100,
      "protocol": "static",
      "ipaddr": "10.100.0.1",
      "netmask": "255.255.255.0"
    },
    "vlan200": {
      "type": "vlan",
      "device": "vlan200",
      "enabled": true,
      "parent": "eth0",
      "vlan_id": 200,
      "protocol": "static",
      "ipaddr": "10.200.0.1",
      "netmask": "255.255.255.0"
    }
  },
  "routes": [
    {
      "destination": "0.0.0.0/0",
      "gateway": "192.168.1.1",
      "metric": 100
    },
    {
      "destination": "10.0.0.0/8",
      "gateway": "10.100.0.254",
      "metric": 200
    },
    {
      "destination": "172.16.0.0/12",
      "gateway": "10.200.0.254",
      "metric": 300
    }
  ]
}
